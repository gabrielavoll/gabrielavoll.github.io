.article
	ul.code-list

		for event_obj, slug in public.curitorial._data
			li
				a(href="/curitorial/#{ slug }")
					.left-side
						image(src= event_obj.walkthrough_mini )
					.col.right-side
						h4= event_obj.date
						h1= event_obj.title
						h4.description= event_obj.description_mini
						h6= "tags: " + event_obj.tags

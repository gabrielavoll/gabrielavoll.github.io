.article
	ul.code-list

		for code_obj, slug in public.code._data
			li
				a(href="/code/#{ slug }")
					.left-side
						image(src= code_obj.walkthrough_mini )
					.col.right-side
						h4= code_obj.date
						h1= code_obj.title
						h4.description= code_obj.description_mini
						h6= "tags: " + code_obj.tags